# MySQL 配置
MYSQL_ROOT_PASSWORD=root
MYSQL_DATABASE=opencoze
MYSQL_USER=coze
MYSQL_PASSWORD=coze123

# Redis 配置
REDIS_AOF_ENABLED=no
REDIS_PORT_NUMBER=6379
REDIS_IO_THREADS=4
ALLOW_EMPTY_PASSWORD=yes

# MinIO 配置
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin123
MINIO_BUCKET=opencoze
MINIO_DEFAULT_BUCKETS=milvus
STORAGE_BUCKET=opencoze

# BGE 嵌入服务配置
BGE_MODEL_NAME=AI-ModelScope/bge-small-zh-v1.5

# 应用服务配置
LISTEN_ADDR=0.0.0.0:8888
SERVER_HOST=localhost:8888
LOG_LEVEL=debug
MAX_REQUEST_BODY_SIZE=1073741824

# 存储配置
STORAGE_TYPE=minio
MINIO_PROXY_ENDPOINT=:8889

# 消息队列配置
COZE_MQ_TYPE=nsq

# Elasticsearch 配置
ES_VERSION=v8

# 向量存储配置
VECTOR_STORE_TYPE=milvus

# 嵌入模型配置
EMBEDDING_TYPE=http
HTTP_EMBEDDING_ADDR=http://bge-embedding:8000
HTTP_EMBEDDING_DIMS=512
EMBEDDING_MAX_BATCH_SIZE=100

# 文件上传配置
FILE_UPLOAD_COMPONENT_TYPE=storage

# OCR 配置
OCR_TYPE=ve
VE_OCR_AK=
VE_OCR_SK=

# 内置聊天模型配置
BUILTIN_CM_TYPE=ark
BUILTIN_CM_ARK_API_KEY=
BUILTIN_CM_ARK_MODEL=
BUILTIN_CM_ARK_BASE_URL=

# 图像处理配置
